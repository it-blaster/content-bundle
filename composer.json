{
    "name": "it-blaster/content-bundle",
    "description": "Symfony2 bundle for site content management, require Propel, Sonata and jstree",
    "type": "symfony-bundle",
    "keywords": ["content", "tree", "structure", "nested set"],
    "homepage": "https://github.com/it-blaster/content-bundle",
    "license" : "MIT",
    "authors": [
        {
            "name": "IT-Blaster",
            "email": "it-blaster@yandex.ru"
        }
    ],
    "require": {
        "symfony/symfony": ">=2.2",
        "propel/propel-bundle": ">=1.0",
        "sonata-project/propel-admin-bundle": ">=1.0",
        "egeloen/ckeditor-bundle": ">=2.0",
        "it-blaster/translation-bundle": "dev-master"
    },
    "autoload": {
        "psr-0": { "Etfostra\\ContentBundle": "" }
    },
    "minimum-stability": "dev",
    "target-dir": "Etfostra/ContentBundle"

}